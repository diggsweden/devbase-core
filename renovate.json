{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "github>diggsweden/.github//renovate-base.json"
  ],
  "mise": {
    "fileMatch": ["(^|/)dot/\\.config/mise/config\\.toml$"]
  },
  "customManagers": [
    {
      "customType": "regex",
      "managerFilePatterns": ["(^|/)dot/\\.config/devbase/versions\\.yaml$"],
      "matchStrings": [
        "(?<depName>\\w+):\\s*(?<currentValue>\\S+)\\s*#\\s*renovate:\\s*datasource=(?<datasource>\\S+)(?:\\s+depName=(?<packageName>\\S+))?(?:\\s+lookupName=(?<lookupName>\\S+))?(?:\\s+extractVersion=(?<extractVersion>\\S+))?"
      ]
    }
  ]
}
